def power_manual(a, n):
    if n == 0:
        return 1.0
    abs_n = abs(n)
    result = 1.0
    for _ in range(abs_n):
        result *= a
    if n < 0:
        return 1.0 / result
    else:
        return result
print("Введіть дійсне число a та ціле число n:")
a = float(input())
n = int(input())
result = power_manual(a, n)
print(f"{result}")
